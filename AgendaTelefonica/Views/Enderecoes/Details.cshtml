@model AgendaTelefonica.Models.Endereco

@{
    ViewData["Title"] = "Detalhes do Endereço";
}



<div>
    <h4 class="text-light">Detalhes do Endereço</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Rua)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Rua)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Numero)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Numero)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Bairro)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Bairro)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Cidade)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Cidade)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Cep)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Cep)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.TipoEndereco)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.TipoEndereco)
        </dd>
        <dt class="col-sm-2 text-light">
            @Html.DisplayNameFor(model => model.Contato)
        </dt>
        <dd class="col-sm-10 text-light">
            @Html.DisplayFor(model => model.Contato.Nome)
        </dd>
    </dl>
</div>
<div class="form-group">
    <a class="btn btn-outline-success" asp-action="Edit" asp-route-id="@Model.Id">Atualizar</a> |
    <a class="btn btn-outline-primary" asp-action="Index">Voltar</a>
</div>
